{
  "name": "d3-bundle-element",
  "version": "1.5.0",
  "description": "polymer element for importing d3.js (v4) as optimized bundle",
  "keywords": [
    "web-component",
    "web-components",
    "polymer",
    "indicator",
    "d3",
    "d3-bundle"
  ],
    "authors": [
    "christophe-g <christophe.geiser@gmail.com>"
  ],
   "repository": "https://github.com/polymerEl/d3-bundle-element",
  "homepage": "https://github.com/polymerEl/d3-bundle-element",
  "main": "build/d3-multi.js",
  "license": "MIT",
  "homepage": "https://github.com/polymerEl/d3-bundle-element",
  "scripts": {
    "prepublish": "rm -rf build && mkdir build && rollup -c && rollup -c rollup.shape.config.js  && rollup -c rollup.topojson.config.js && rollup -c rollup.multi.config.js && rollup -c rollup.small.config.js && uglifyjs build/d3.js -c -m -o build/d3.min.js && uglifyjs build/d3-shape.js -c -m -o build/d3-shape.min.js  && uglifyjs build/d3-multi.js -c -m -o build/d3-multi.min.js && uglifyjs build/d3-small.js -c -m -o build/d3-small.min.js && uglifyjs build/topojson.js -c -m -o build/topojson.min.js"
  },
  "dependencies": {
    "d3-array": "^1.0.1",
    "d3-axis": "^1.0.3",
    "d3-brush": "^1.0.3",
    "d3-chord": "^1.0.2",
    "d3-collection": "^1.0.1",
    "d3-color": "^1.0.1",
    "d3-dispatch": "^1.0.1",
    "d3-drag": "^1.0.1",
    "d3-dsv": "^1.0.3",
    "d3-ease": "^1.0.1",
    "d3-force": "^1.0.2",
    "d3-format": "^1.0.2",
    "d3-geo": "^1.2.5",
    "d3-hierarchy": "^1.0.2",
    "d3-interpolate": "^1.1.1",
    "d3-path": "^1.0.2",
    "d3-polygon": "^1.0.1",
    "d3-quadtree": "^1.0.1",
    "d3-queue": "^3.0.3",
    "d3-random": "^1.0.1",
    "d3-request": "^1.0.2",
    "d3-scale": "^1.0.3",
    "d3-selection": "^1.0.2",
    "d3-shape": "^1.0.3",
    "d3-time": "^1.0.4",
    "d3-time-format": "^2.0.2",
    "d3-timer": "^1.0.3",
    "d3-transition": "^1.0.2",
    "d3-voronoi": "^1.0.2",
    "d3-zoom": "^1.0.3",
    "d3-tile": "^0.0.3",
    "topojson": "^1.6.0",
    "d3-selection-multi": "^1.0.0",
    "d3-hexbin" : "^0.2.0",
    "d3-svg-legend": "https://github.com/christophe-g/d3-legend#mergeSelection"
  },
  "devDependencies": {
    "json2module": "0.0",
    "package-preamble": "0.0",
    "rimraf": "2",
    "rollup": "0.36",
    "rollup-plugin-ascii": "0.0",
    "rollup-plugin-node-resolve": "2",
    "tape": "4",
    "uglify-js": "2",
    "rollup-plugin-node-resolve": "^1.5.0",
    "uglify-js": "^2.6.2"
  }
}
